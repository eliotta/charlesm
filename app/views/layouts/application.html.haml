!!!
%html
  %head
    %title Charlesm
    = stylesheet_link_tag    "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    .welcome
      - if current_user
        Welcome #{current_user.email}
      - else
        = link_to "Sign In", new_user_session_url
    = yield
